<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/llm/memory/"
     xml:base="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/llm/memory/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cat="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/">

    <owl:Ontology rdf:about="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/llm/memory">
        <rdfs:label>LLM Memory Rails Engine</rdfs:label>
        <rdfs:comment>Rails Engine for collecting facts from llm-state, Rails logs, and OpenTelemetry, </rdfs:comment>
    </owl:Ontology>

    <!-- Repository Individual -->
    <owl:NamedIndividual rdf:about="#LlmMemory">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#RaySwarmEngine"/>
        <cat:repositoryName>llm-memory</cat:repositoryName>
        <cat:repositoryUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://github.com/laquereric/llm-memory</cat:repositoryUrl>
        <cat:gemName>llm-memory</cat:gemName>
        <cat:engineNamespace>LlmMemory</cat:engineNamespace>
        <cat:purpose>LLM Memory Rails Engine</cat:purpose>
        <cat:techStack>Ruby, Rails Engine</cat:techStack>
        <cat:dependsOn rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/rails/#Rails"/>
        <cat:dependsOn rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/view_component/#ViewComponent"/>
        <cat:provides rdf:resource="#BronzeFact"/>
        <cat:provides rdf:resource="#GoldInsight"/>
        <cat:provides rdf:resource="#SilverFact"/>
    </owl:NamedIndividual>

    <!-- Component: BronzeFact -->
    <owl:NamedIndividual rdf:about="#BronzeFact">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>BronzeFact</rdfs:label>
        <rdfs:comment>Model backed by table: lm_bronze_facts</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Component: GoldInsight -->
    <owl:NamedIndividual rdf:about="#GoldInsight">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>GoldInsight</rdfs:label>
        <rdfs:comment>Model backed by table: lm_gold_insights</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Component: SilverFact -->
    <owl:NamedIndividual rdf:about="#SilverFact">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>SilverFact</rdfs:label>
        <rdfs:comment>Model backed by table: lm_silver_facts</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>
